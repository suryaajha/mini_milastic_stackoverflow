<h2>Sign up</h2>

<%= bootstrap_form_for(@user, url: user_registration_path) do |f| %>
  <%= f.text_field :name %>
  <%= f.text_field :username %>
  <%= f.email_field :email %>
  <%= f.password_field :password %>
  <%= f.password_field :password_confirmation %>
  <%= f.text_area :bio %>
  <%= f.submit "Sign Up" %>
<% end %>

<%= render "devise/shared/links" %>
